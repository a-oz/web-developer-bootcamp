<%- include("partials/header.ejs") %>

<h1>The post page</h1>

<ol>
    <% for (let i = 0; i < posts.length; ++i) { %>
        <li>
            Title: <%= posts[i].title %>, Author: <%= posts[i].author %>
        </li>
    <% } %>
</ol>

<table>
    <thead>
        <th>Title</th>
        <th>Author</th>
    </thead>

    <% posts.forEach((post) => { %>
        <tr>
            <td><%= post.title %></td>
            <td><%= post.author %></td>
        </tr>
    <% }) %>
</table>

<%- include("partials/footer.ejs") %>
